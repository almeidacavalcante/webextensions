{

  "manifest_version": 2,
  "name": "Lucifer Plug-in",
  "version": "1.2",

  "description": "Agilize seu trabalho com o poder do LÃºcifer!",

  "icons": {
    "48": "icons/lucifer-48.png"
  },

  "background":
  {
      "scripts": ["thirdParty/jquery.js", "background.js"]
  },

  "browser_action":{
    "default_icon": "img/penny.png",
    "default_title": "Notifier",
    "default_popup": "index.html"
  },

  "permissions": [
    "desktopCapture",
    "notifications",
    "tabs",
    "storage"
  ],

  "web_accessible_resources": [
    "img/*.png"
  ],

  "content_scripts": [
    {
      "matches": ["https://srvapp04.mp.rn.gov.br:8443/sitesOrgaosMP/*"],
      "js": ["inscricaoCeaf.js"]
    },
    {
      "matches": ["https://www.google.com.br/*"],
      "js": ["otrsScript.js"]
    },
    {
      "matches": ["http://srv-helpdesk.mp.rn.gov.br/otrs/index.pl*"],
      "js": ["login.js"]
    },
    {
    "matches":["https://www.google.com.br/*"],
    "js":["thirdParty/jquery.js"],
    "run_at": "document_end"
  }
  ]
}
